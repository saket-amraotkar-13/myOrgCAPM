{
  "name": "myorg",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "dependencies": {
    "@sap-cloud-sdk/connectivity": "^3",
    "@sap-cloud-sdk/http-client": "^3.26.4",
    "@sap-cloud-sdk/resilience": "^3",
    "@sap/cds": "^9",
    "@sap/xsenv": "^6.0.0",
    "exceljs": "^4.4.0",
    "express": "^4",
    "node-fetch": "^2.7.0",
    "node-jose": "^2.2.0",
    "sap-cf-mailer": "^0.0.5",
    "save": "^2.9.0"
  },
  "devDependencies": {
    "@cap-js/sqlite": "^2",
    "cds-plugin-ui5": "^0.13.0"
  },
  "scripts": {
    "start": "cds-serve",
    "watch-ui-employee": "cds watch --open uiemployee/index.html?sap-ui-xx-viewCache=false --livereload false"
  },
  "cds": {
    "requires": {
      "[production]": {
        "auth": {
          "kind": "mocked"
        }
      },
      "northwind": {
        "kind": "odata-v2",
        "model": "srv/external/northwind",
        "credentials": {
          "destination": "Northwind",
          "path": "/v2/northwind/northwind.svc"
        }
      }
    }
  },
  "private": true,
  "workspaces": [
    "app/*"
  ],
  "sapux": [
    "app/ui-employee",
    "app/ui-employee"
  ]
}
